{
  "ignore": [
    "**/node_modules/**",
    "**/lib/**",
    "**/**-min-**",
    "**/**-min.**",
    "**/**.min.**",
    "**/**jquery.?(ui|effects)-*.*.?(*).?(cs|j)s",
    "**/**jquery-*.*.?(*).?(cs|j)s",
    "**/**jasmine.js",
    "**/strings/cultures/**",
    "**/sass/bourbon/**",
    "**/sass/neat/**",
    "**/breakingchanges/**",
    "**/docs/**",
    "**/typings/**"
